<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.personalplanner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/App_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".Timer.ViewTime"></activity>
        <activity android:name=".Timer.StopWatchScreen"/>
        <activity android:name=".Timer.HomeScreen"/>


        <activity android:name=".TODO_List.UpdateAndDelete"></activity>
        <activity android:name=".TODO_List.NewTaskAct"/>
        <activity android:name=".TODO_List.MainActivity_Todo"/>


        <activity android:name=".Alarm.HomeScreen"></activity>
        <activity android:name=".Alarm.ViewAlarm" />
        <activity android:name=".Alarm.UpdateAlarm" />
        <activity android:name=".Alarm.MainActivity"/>


        <activity android:name=".MainActivity_PP">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>